{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "C#: TestApp",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/TestApp/bin/Debug/net8.0/TestApp.dll",
            "cwd": "${workspaceFolder}/TestApp",
            "stopAtEntry": false,
            "projectPath": "${workspaceFolder}/TestApp/TestApp.csproj",
            "launchConfigurationId": "TargetFramework=;TestApp",
            "args": [
                "-datpath",
                "${env:HOME}/Development/Signatures/NoIntro/DAT",
                "-dbpath",
                "${env:HOME}/Development/Signatures/NoIntro/DB",
                "-outpath",
                "${env:HOME}/Development/Signatures/NoIntro/Output",
            ]
        }
    ]
}